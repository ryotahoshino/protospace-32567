<%= form_with model: prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title%>
  </div>

  <div class="field">
    <%= f.label :catch_copy %><br />
    <%= f.text_area :catch_copy, class: "post" %>
  </div>

  <div class="field">
    <%= f.label :consept %><br />
    <%= f.text_area :concept, class: "post" %>
  </div>

  <div class="field">
    <%= f.label :image %><br />
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>
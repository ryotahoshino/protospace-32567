<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <P><%= @name %>さんの情報</p>
      </h2>
      <table class="table">
        <tbody>
        <% @prototypes.each do |prototype| %>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= "ユーザー名" %></td>
          </tr>
          <tr>
            <th class="table__col1">プロフィール</th>
            <td class="table__col2"><%= prototypoe.name %></td>
          </tr>
          <tr>
            <th class="table__col1">所属</th>
            <td class="table__col2"><%= prototype.occupation %></td>
          </tr>
          <tr>
            <th class="table__col1">役職</th>
            <td class="table__col2"><%= prototype.position %></td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= user.prototype %>
      </h2>
      <div class="user__card">
        <%# 部分テンプレートでそのユーザーが投稿したプロトタイプ投稿一覧を表示する %>
        <%= render partial: "prototype", collection: @prototypes%>
      </div>
    </div>
  </div>
</div>